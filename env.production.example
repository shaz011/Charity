# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Your Supabase project URL (production)
VITE_SUPABASE_URL=https://your-production-project-id.supabase.co

# Your Supabase anonymous/public key (production)
VITE_SUPABASE_ANON_KEY=your-production-anon-key-here

# Disable debug logging in production
VITE_DEBUG=false

# Production API endpoint (if different)
VITE_API_URL=https://your-api-endpoint.com
